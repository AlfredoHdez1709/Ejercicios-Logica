<Window x:Class="Proyecto_Euler.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Proyecto_Euler"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="btnMultiplos" Content="Múltiplos de 3 y 5" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="150" Height="30" Click="btnMultiplos_Click"/>
        <Button x:Name="btnFactoriales" Content="Factoriales" HorizontalAlignment="Left" Margin="10,45,0,0" VerticalAlignment="Top" Width="150" Height="30" Click="btnFactoriales_Click"/>
        <Button x:Name="btnTernasPitagoras" Content="Ternas pitagóricas" HorizontalAlignment="Left" Margin="10,80,0,0" VerticalAlignment="Top" Width="150" Height="30" Click="btnTernasPitagoras_Click"/>
        <Button x:Name="btnPalindromos" Content="Palíndromos" HorizontalAlignment="Left" Margin="10,115,0,0" VerticalAlignment="Top" Width="150" Height="30" Click="btnPalindromos_Click"/>
        <Button x:Name="btnPrimos" Content="Primos" HorizontalAlignment="Left" Margin="10,150,0,0" VerticalAlignment="Top" Width="150" Height="30" Click="btnPrimos_Click"/>

    </Grid>
</Window>
